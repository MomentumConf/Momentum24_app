{
    "buildCommand": "flutter build web --wasm --dart-define=SENTRY_DSN=$SENTRY_DSN --dart-define=SANITY_PROJECT_ID=$SANITY_PROJECT_ID --release",
    "outputDirectory": "build/web/",
    "installCommand": "flutter pub get",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ]
}